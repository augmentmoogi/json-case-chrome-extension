{
  "manifest_version": 3,
  "name": "JSON to Underscore Case",
  "version": "1.0",
  "description": "Converts JSON properties to underscore case",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["clipboardWrite", "clipboardRead", "activeTab"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
